{
  "name": "__MSG_extName__",
  "version": "0",
  "version_name": "alpha",
  "description": "__MSG_extDesc__",
  "author": "Jess Austin",
  "homepage_url": "https://github.com/jessaustin/hatrack",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_actDefault__"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "Command+Shift+H"
      }
    }
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "omnibox": {
    "keyword": "hatrack"
  },
  "default_locale": "en",
  "content_security_policy": "default-src 'self'; style-src 'unsafe-inline'",
  "incognito": "split",
  "minimum_chrome_version": "1",
  "permissions": [
    "storage"
  ],
  "optional_permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*/*"
  ],
  "manifest_version": 2
}
